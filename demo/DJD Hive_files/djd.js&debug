/* 221  */ 					visible = $.map(classname_alert, function (val) { return '.'+val+':not(.hidden)' });
/* 222  */ 				if(destroy !== true && $gallery.find('.gallery > .'+classname_tag).length > 0) {
/* 223  */ 					$gallery.find(hidden.join(','))
/* 224  */ 						.removeClass('hidden')
