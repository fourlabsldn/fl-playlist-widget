/* search.js */

/* 1   */ var DJDSearch = (function ($) {
/* 2   */ 	"use strict";
/* 3   */ 	var timeout = null,
/* 4   */ 		$menu,
/* 5   */ 		$form,
/* 6   */ 		$field,
/* 7   */ 		hide = function () {
/* 8   */ 			$form.removeClass('active');
/* 9   */ 			$menu.addClass('hidden');
/* 10  */ 			$field.val('');
/* 11  */ 		},
/* 12  */ 		scroll = function () {
/* 13  */ 			var $item = $menu.find('.focus'),
/* 14  */ 				menuHeight = $menu.outerHeight(),
/* 15  */ 				diff = $item.position().top-menuHeight;
/* 16  */ 			// below visible area
/* 17  */ 			if(diff >= 0) {
/* 18  */ 				$menu.scrollTop($menu.scrollTop()+diff+$item.outerHeight()+10);
/* 19  */ 				return;
/* 20  */ 			}
/* 21  */ 			// above visible area
/* 22  */ 			if(diff*(-1) > menuHeight) {
/* 23  */ 				$menu.scrollTop($menu.scrollTop()+diff+menuHeight-10);
/* 24  */ 				return;
/* 25  */ 			}
/* 26  */ 		},
/* 27  */ 		performSearch = function () {
/* 28  */ 			if($field.val().length < 2 || timeout !== null) {
/* 29  */ 				return false;
/* 30  */ 			}
/* 31  */ 
/* 32  */ 			$form.addClass('active');
/* 33  */ 
/* 34  */ 			timeout = window.setTimeout(function () {
/* 35  */ 				var term = $field.val();
/* 36  */ 				timeout = null;
/* 37  */ 				$.ajax({
/* 38  */ 					type: 'post',
/* 39  */ 					url: '/?action=search',
/* 40  */ 					dataType: 'json',
/* 41  */ 					data: {
/* 42  */ 						term: term
/* 43  */ 					},
/* 44  */ 					success: function (results) {
/* 45  */ 						var html_item = '<li>'+
/* 46  */ 								'<a href="%url%">'+
/* 47  */ 									'<i class="fa fa-%icon%"></i>'+
/* 48  */ 									'<span>'+
/* 49  */ 										'<strong>%title%</strong><br />'+
/* 50  */ 										'%attribute%: %term%'+

/* search.js */

/* 51  */ 									'</span>'+
/* 52  */ 								'</a>'+
/* 53  */ 							'</li>',
/* 54  */ 							html_separator = '<li class="divider" />',
/* 55  */ 							len = results.length;
/* 56  */ 
/* 57  */ 						location.hash = '';
/* 58  */ 
/* 59  */ 						$menu.empty().removeClass('hidden');
/* 60  */ 
/* 61  */ 						if(len === 0) {
/* 62  */ 							$menu.append('<li><a><strong>No search results</strong><br />Make sure that all words are spelled correctly.</a></li>');
/* 63  */ 							return false;
/* 64  */ 						}
/* 65  */ 
/* 66  */ 						// show dropdown
/* 67  */ 						$.each(results, function (n, result) {
/* 68  */ 							// highlight term
/* 69  */ 							var highlight = new RegExp('('+term+')', 'gi');
/* 70  */ 							result.term = result.term.replace(highlight, '<strong>$1</strong>');
/* 71  */ 
/* 72  */ 							$menu.append(
/* 73  */ 								html_item.replace(/%(\w+)%/g, function($0, $1) {
/* 74  */ 									return result[$1] !== undefined ? result[$1] : '%'+$1+'%';
/* 75  */ 								})+(n < len-1 ? html_separator : '')
/* 76  */ 							);
/* 77  */ 						});
/* 78  */ 
/* 79  */ 						// focus first result
/* 80  */ 						$menu.find('li:first-child').addClass('focus');
/* 81  */ 					}
/* 82  */ 				});
/* 83  */ 			}, 500);
/* 84  */ 		};
/* 85  */ 
/* 86  */ 	return {
/* 87  */ 		init: function (selector) {
/* 88  */ 			var offset;
/* 89  */ 
/* 90  */ 			$form = $(selector);
/* 91  */ 			$menu = $('<ul class="dropdown-menu hidden" role="menu" />').appendTo($form);
/* 92  */ 			$field = $('input[type=text],input[type=search]', selector);
/* 93  */ 
/* 94  */ 			offset = $form.offset();
/* 95  */ 			$menu.css({
/* 96  */ 				left: offset.left+$form.outerWidth(),
/* 97  */ 				top: offset.top
/* 98  */ 			});
/* 99  */ 
/* 100 */ 			$field.unbind('keydown').keydown(function (e) {

/* search.js */

/* 101 */ 				var $focus = null;
/* 102 */ 
/* 103 */ 				switch(e.keyCode) {
/* 104 */ 					case 27: // ESC
/* 105 */ 						hide();
/* 106 */ 						break;
/* 107 */ 					case 37: // arrow left
/* 108 */ 					case 38: // arrow up
/* 109 */ 						e.preventDefault();
/* 110 */ 						$focus = $menu.find('.focus').prevAll(':not(.divider)').first();
/* 111 */ 						if($focus.length === 0) {
/* 112 */ 							$focus = $menu.find('li:last-child');
/* 113 */ 						}
/* 114 */ 						$menu.find('.focus').removeClass('focus');
/* 115 */ 						$focus.addClass('focus');
/* 116 */ 						scroll();
/* 117 */ 						break;
/* 118 */ 					case 39: // arrow right
/* 119 */ 					case 40: // arrow down
/* 120 */ 						e.preventDefault();
/* 121 */ 						$focus = $menu.find('.focus').nextAll(':not(.divider)').first();
/* 122 */ 						if($focus.length === 0) {
/* 123 */ 							$focus = $menu.find('li:first-child');
/* 124 */ 						}
/* 125 */ 						$menu.find('.focus').removeClass('focus');
/* 126 */ 						$focus.addClass('focus');
/* 127 */ 						scroll();
/* 128 */ 						break;
/* 129 */ 					case 13: // enter
/* 130 */ 						e.preventDefault();
/* 131 */ 						if($menu.find('.focus a').attr('href') === undefined) {
/* 132 */ 							return false;
/* 133 */ 						}
/* 134 */ 						window.location.href = $menu.find('.focus a').attr('href');
/* 135 */ 						break;
/* 136 */ 					default:
/* 137 */ 						performSearch();
/* 138 */ 						break;
/* 139 */ 				}
/* 140 */ 			});
/* 141 */ 
/* 142 */ 			$form.submit(function (e) {
/* 143 */ 				e.preventDefault();
/* 144 */ 			});
/* 145 */ 			$('button', selector).click(function (e) {
/* 146 */ 				e.preventDefault();
/* 147 */ 			});
/* 148 */ 
/* 149 */ 			$(document).click(function (e) {
/* 150 */ 				if($(e.target).parents(selector).length === 0) {

/* search.js */

/* 151 */ 					hide();
/* 152 */ 				}
/* 153 */ 			});
/* 154 */ 		}
/* 155 */ 	};
/* 156 */ }(jQuery));

;
/* djd.js */

/* 1    */ $.fn.bindFirst = function(name, fn) {
/* 2    */ 	"use strict";
/* 3    */ 	// bind as you normally would
/* 4    */ 	// don't want to miss out on any jQuery magic
/* 5    */ 	this.on(name, fn);
/* 6    */ 
/* 7    */ 	// Thanks to a comment by @Martin, adding support for
/* 8    */ 	// namespaced events too.
/* 9    */ 	this.each(function() {
/* 10   */ 		var handlers = $._data(this, 'events')[name.split('.')[0]],
/* 11   */ 		// take out the handler we just inserted from the end
/* 12   */ 			handler = handlers.pop();
/* 13   */ 		// move it at the beginning
/* 14   */ 		handlers.splice(0, 0, handler);
/* 15   */ 	});
/* 16   */ };
