/* 1   */ (function (root, factory) {
/* 2   */ 
/* 3   */     'use strict';
/* 4   */ 
/* 5   */     if (typeof define === 'function' && define.amd) {
/* 6   */         // AMD environment
/* 7   */         define('notify', [], function () {
/* 8   */             return factory(root, document);
/* 9   */         });
/* 10  */     } else if (typeof exports === 'object') {
/* 11  */         // CommonJS environment
/* 12  */         module.exports = factory(root, document);
/* 13  */     } else {
/* 14  */         // Browser environment
/* 15  */         root.Notify = factory(root, document);
/* 16  */     }
/* 17  */ 
/* 18  */ }(window, function (w, d) {
/* 19  */ 
/* 20  */     'use strict';
/* 21  */ 
/* 22  */     function Notify(title, options) {
/* 23  */ 
/* 24  */         if (typeof title !== 'string') {
/* 25  */             throw new Error('Notify(): first arg (title) must be a string.');
/* 26  */         }
/* 27  */ 
/* 28  */         this.title = title;
/* 29  */ 
/* 30  */         this.options = {
/* 31  */             icon: '',
/* 32  */             body: '',
/* 33  */             tag: '',
/* 34  */             notifyShow: null,
/* 35  */             notifyClose: null,
/* 36  */             notifyClick: null,
/* 37  */             notifyError: null,
/* 38  */             permissionGranted: null,
/* 39  */             permissionDenied: null,
/* 40  */             timeout: null
/* 41  */         };
/* 42  */ 
/* 43  */         this.permission = null;
/* 44  */ 
/* 45  */         if (!Notify.isSupported()) {
/* 46  */             return;
/* 47  */         }
/* 48  */ 
/* 49  */         //User defined options for notification content
/* 50  */         if (typeof options === 'object') {

/* notify.js */

/* 51  */ 
/* 52  */             for (var i in options) {
/* 53  */                 if (options.hasOwnProperty(i)) {
/* 54  */                     this.options[i] = options[i];
/* 55  */                 }
/* 56  */             }
/* 57  */ 
/* 58  */             //callback when notification is displayed
/* 59  */             if (typeof this.options.notifyShow === 'function') {
/* 60  */                 this.onShowCallback = this.options.notifyShow;
/* 61  */             }
/* 62  */ 
/* 63  */             //callback when notification is closed
/* 64  */             if (typeof this.options.notifyClose === 'function') {
/* 65  */                 this.onCloseCallback = this.options.notifyClose;
/* 66  */             }
/* 67  */ 
/* 68  */             //callback when notification is clicked
/* 69  */             if (typeof this.options.notifyClick === 'function') {
/* 70  */                 this.onClickCallback = this.options.notifyClick;
/* 71  */             }
/* 72  */ 
/* 73  */             //callback when notification throws error
/* 74  */             if (typeof this.options.notifyError === 'function') {
/* 75  */                 this.onErrorCallback = this.options.notifyError;
/* 76  */             }
/* 77  */         }
/* 78  */     }
/* 79  */ 
/* 80  */     // return true if the browser supports HTML5 Notification
/* 81  */     Notify.isSupported = function () {
/* 82  */         if ('Notification' in w) {
/* 83  */             return true;
/* 84  */         }
/* 85  */         return false;
/* 86  */     };
/* 87  */ 
/* 88  */     // returns true if the permission is not granted
/* 89  */     Notify.needsPermission = function () {
/* 90  */         if (Notify.isSupported() && Notification.permission === 'granted') {
/* 91  */             return false;
/* 92  */         }
/* 93  */         return true;
/* 94  */     };
/* 95  */ 
/* 96  */     // asks the user for permission to display notifications.  Then calls the callback functions is supplied.
/* 97  */     Notify.requestPermission = function (onPermissionGrantedCallback, onPermissionDeniedCallback) {
/* 98  */         if (Notify.isSupported()) {
/* 99  */             w.Notification.requestPermission(function (perm) {
/* 100 */                 switch (perm) {

/* notify.js */

/* 101 */                     case 'granted':
/* 102 */                         if (typeof onPermissionGrantedCallback === 'function') {
/* 103 */                             onPermissionGrantedCallback();
/* 104 */                         }
/* 105 */                         break;
/* 106 */                     case 'denied':
/* 107 */                         if (typeof onPermissionDeniedCallback === 'function') {
/* 108 */                             onPermissionDeniedCallback();
/* 109 */                         }
/* 110 */                         break;
/* 111 */                 }
/* 112 */             });
/* 113 */         }
/* 114 */     };
/* 115 */ 
/* 116 */ 
/* 117 */     Notify.prototype.show = function () {
/* 118 */         var that = this;
/* 119 */ 
/* 120 */         if (!Notify.isSupported()) {
/* 121 */             return;
/* 122 */         }
/* 123 */ 
/* 124 */         this.myNotify = new Notification(this.title, {
/* 125 */             'body': this.options.body,
/* 126 */             'tag' : this.options.tag,
/* 127 */             'icon' : this.options.icon
/* 128 */         });
/* 129 */ 
/* 130 */         if (this.options.timeout && !isNaN(this.options.timeout)) {
/* 131 */             setTimeout(this.close.bind(this), this.options.timeout * 1000);
/* 132 */         }
/* 133 */ 
/* 134 */         this.myNotify.addEventListener('show', this, false);
/* 135 */         this.myNotify.addEventListener('error', this, false);
/* 136 */         this.myNotify.addEventListener('close', this, false);
/* 137 */         this.myNotify.addEventListener('click', this, false);
/* 138 */     };
/* 139 */ 
/* 140 */     Notify.prototype.onShowNotification = function (e) {
/* 141 */         if (this.onShowCallback) {
/* 142 */             this.onShowCallback(e);
/* 143 */         }
/* 144 */     };
/* 145 */ 
/* 146 */     Notify.prototype.onCloseNotification = function () {
/* 147 */         if (this.onCloseCallback) {
/* 148 */             this.onCloseCallback();
/* 149 */         }
/* 150 */         this.destroy();

/* notify.js */

/* 151 */     };
/* 152 */ 
/* 153 */     Notify.prototype.onClickNotification = function () {
/* 154 */         if (this.onClickCallback) {
/* 155 */             this.onClickCallback();
/* 156 */         }
/* 157 */     };
/* 158 */ 
/* 159 */     Notify.prototype.onErrorNotification = function () {
/* 160 */         if (this.onErrorCallback) {
/* 161 */             this.onErrorCallback();
/* 162 */         }
/* 163 */         this.destroy();
/* 164 */     };
/* 165 */ 
/* 166 */     Notify.prototype.destroy = function () {
/* 167 */         this.myNotify.removeEventListener('show', this, false);
/* 168 */         this.myNotify.removeEventListener('error', this, false);
/* 169 */         this.myNotify.removeEventListener('close', this, false);
/* 170 */         this.myNotify.removeEventListener('click', this, false);
/* 171 */     };
/* 172 */ 
/* 173 */     Notify.prototype.close = function () {
/* 174 */         this.myNotify.close();
/* 175 */     };
/* 176 */ 
/* 177 */     Notify.prototype.handleEvent = function (e) {
/* 178 */         switch (e.type) {
/* 179 */         case 'show':
/* 180 */             this.onShowNotification(e);
/* 181 */             break;
/* 182 */         case 'close':
/* 183 */             this.onCloseNotification(e);
/* 184 */             break;
/* 185 */         case 'click':
/* 186 */             this.onClickNotification(e);
/* 187 */             break;
/* 188 */         case 'error':
/* 189 */             this.onErrorNotification(e);
/* 190 */             break;
/* 191 */         }
/* 192 */     };
/* 193 */ 
/* 194 */     return Notify;
/* 195 */ 
/* 196 */ }));
