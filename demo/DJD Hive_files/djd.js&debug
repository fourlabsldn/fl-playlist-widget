/* 248  */ 					if (!$modal.is('.in') && +keyDownAt > +lastKeyUpAt) {
/* 249  */ 						down = false;
/* 250  */ 						$modal.modal({

/* djd.js */

/* 251  */ 							remote: '/?action=game'
/* 252  */ 						});
/* 253  */ 					}
/* 254  */ 				}, 1500);
/* 255  */ 			});
/* 256  */ 
