/* 105 */ 					.find('.notify.new').removeClass('new');
/* 106 */ 			});
/* 107 */ 			DJD.beautifyDate('#navbar .notify .date');
/* 108 */ 			if(no === 0) {
/* 109 */ 				return;
/* 110 */ 			}
/* 111 */ 			// update title
/* 112 */ 			var title = document.title.split('|');
/* 113 */ 			title[0] = title[0]+' ('+no+') ';
/* 114 */ 			document.title = title.join('|');
/* 115 */ 		}
