/* 425  */ 				return false;
/* 426  */ 			}
/* 427  */ 			$fields.on('change', checkPasswordValidity);
/* 428  */ 		},
/* 429  */ 		uploadArea: function (selector) {
/* 430  */ 			var $fileupload = $(selector),
/* 431  */ 				url = $fileupload.attr('action');
/* 432  */ 
/* 433  */ 			$fileupload.fileupload({
/* 434  */ 				url: url,
/* 435  */ 				formData: $fileupload.serializeArray(),
/* 436  */ 				autoUpload: true,
/* 437  */ 				disableImageResize: false,
/* 438  */ 				imageMaxWidth: 1680,
/* 439  */ 				imageMaxHeight: 1200,
/* 440  */ 				dropZone: $fileupload,
/* 441  */ 				previewMaxWidth: 100,
/* 442  */ 				previewMaxHeight: 100,
/* 443  */ 				previewCrop: true
/* 444  */ 			});
/* 445  */ 
/* 446  */ 			// load existing files
/* 447  */ 			$fileupload.addClass('fileupload-processing');
/* 448  */ 			$.ajax({
/* 449  */ 				url: url,
/* 450  */ 				data: $fileupload.serializeArray(),

/* djd.js */

/* 451  */ 				dataType: 'json',
/* 452  */ 				context: $fileupload.get(0)
/* 453  */ 			}).always(function () {
/* 454  */ 				$(this).removeClass('fileupload-processing');
/* 455  */ 			}).done(function (result) {
/* 456  */ 				$(this)
/* 457  */ 					.fileupload('option', 'done')
/* 458  */ 					.call(this, $.Event('done'), {result: result});
/* 459  */ 			});
/* 460  */ 
/* 461  */ 			// toggle checkbox
/* 462  */ 			$fileupload.find('thead .toggle').change(function (e) {
/* 463  */ 				$fileupload.find('tbody .toggle').prop(
/* 464  */ 					'checked',
/* 465  */ 					$(e.currentTarget).is(':checked')
/* 466  */ 				);
/* 467  */ 			});
/* 468  */ 		},
/* 469  */ 		hoverTitle: function (selector) {
/* 470  */ 			$('#modal').off('scroll.hoverTitle');
/* 471  */ 			if(typeof $.fn.visible !== 'function') {
/* 472  */ 				return false;
/* 473  */ 			}
/* 474  */ 			$(selector).each(function () {
/* 475  */ 				var $box = $(this),
/* 476  */ 					$title = $box.find('.box-title'),
/* 477  */ 					$content = $box.find('.gallery'),
/* 478  */ 					$row = $box.closest('.row'),
/* 479  */ 					$prev = $row.prev(),
/* 480  */ 					hovering = false,
/* 481  */ 					padding = $title.outerHeight();
/* 482  */ 				$title.css('width', $title.innerWidth()+'px');
/* 483  */ 				$('#modal').on('scroll.hoverTitle', $.throttle(400, function () {
/* 484  */ 					var content_visible = $content.visible(true),
/* 485  */ 						prev_visible = $prev.visible(true);
/* 486  */ 					if((content_visible === false || prev_visible === true) && hovering === true) {
/* 487  */ 						$title.css({
/* 488  */ 							position: 'relative',
/* 489  */ 							left: 0,
/* 490  */ 							top: 0
/* 491  */ 						});
/* 492  */ 						$box.css('padding-top', 0);
/* 493  */ 						hovering = false;
/* 494  */ 					} else if(content_visible === true && prev_visible === false) {
/* 495  */ 						$title.css({
/* 496  */ 							position: 'absolute',
/* 497  */ 							left: '15px',
/* 498  */ 							top: ($('#modal').scrollTop()-$row.position().top-190)+'px'
/* 499  */ 						});
/* 500  */ 						$box.css('padding-top', padding+'px');

/* djd.js */

/* 501  */ 						hovering = true;
/* 502  */ 					}
/* 503  */ 				}));
/* 504  */ 			});
/* 505  */ 			$('#modal').trigger('scroll.hoverTitle');
/* 506  */ 		},
/* 507  */ 		imageActionsDestroy: function (selector) {
/* 508  */ 			var $gallery = $(selector);
/* 509  */ 			$gallery
/* 510  */ 				.off('click.imageActions mousemove.imageActions mouseleave.imageActions')
/* 511  */ 				.find('.untag-all, .select-all, .select-images, .select-documents, .tag-photo, '+
/* 512  */ 					'.tag-floorplan, .tag-epc, .tag-exposures, .tag-editing, .tag-contact-sheet').off('click');
/* 513  */ 
/* 514  */ 			$(document).off('keypress.imageActions');
/* 515  */ 			imageActionsAlerts($gallery, true);
/* 516  */ 		},
/* 517  */ 		imageActions: function (selector) {
/* 518  */ 			var $galleries = $(selector);
/* 519  */ 			$(document).off('keypress.imageActions');
/* 520  */ 			$galleries.each(function (n, gallery) {
/* 521  */ 				var $gallery = $(gallery),
/* 522  */ 					exposure_group = 0,
/* 523  */ 					pretyped = $gallery.find('.gallery').hasClass('pre-typed'),
/* 524  */ 					// visible?
/* 525  */ 					$content = $gallery.find('.gallery'),
/* 526  */ 					$row = $gallery.closest('.row'),
/* 527  */ 					$prev = $row.prev('.row').find('.gallery');
/* 528  */ 				// when created scan for existing exposure groups
/* 529  */ 				$gallery.find('.exposures').each(function () {
/* 530  */ 					var data = $(this).data('exposure-group')+1;
/* 531  */ 					if(data > exposure_group) {
/* 532  */ 						exposure_group = data;
/* 533  */ 					}
/* 534  */ 				});
/* 535  */ 				// when created scan for existing tags
/* 536  */ 				imageActionsAlerts($gallery);
/* 537  */ 				// don't perform standard action when a button is hit (details, preview etc.)
/* 538  */ 				$gallery.on('click.imageActions', '.gallery > li a', function (e) {
/* 539  */ 					var $target = $(e.target);
/* 540  */ 					if(!$target.hasClass('btn') && !$target.parent().hasClass('btn')) {
/* 541  */ 						e.preventDefault();
/* 542  */ 						e.stopPropagation();
/* 543  */ 					}
/* 544  */ 				});
/* 545  */ 				// don't perform standard action when item is disabled
/* 546  */ 				$gallery.on('click.imageActions', '.gallery > li:not(.disabled) a', function (e) {
/* 547  */ 					var $target = $(e.target);
/* 548  */ 					if(!$target.hasClass('btn') && !$target.parent().hasClass('btn')) {
/* 549  */ 						$(this).closest('li').toggleClass('selected');
/* 550  */ 					}

/* djd.js */

/* 551  */ 				});
/* 552  */ 				// remove type when clicking type icon
/* 553  */ 				$gallery.on('click.imageActions', '.gallery > li:not(.disabled) .typebox .fa', function (e) {
/* 554  */ 					var $this = $(this),
/* 555  */ 						width = 29,
/* 556  */ 						height = 26,
/* 557  */ 						left = ($this.outerWidth()-width)/2,
/* 558  */ 						top = ($this.outerHeight()-height)/2;
/* 559  */ 
/* 560  */ 					if(e.offsetX >= left && e.offsetX <= left+width && e.offsetY >= top && e.offsetY <= top+height) {
/* 561  */ 						e.preventDefault();
/* 562  */ 						e.stopPropagation();
/* 563  */ 						$this.closest('.typed')
/* 564  */ 							.removeClass('typed photo floor-plan epc');
/* 565  */ 					}
/* 566  */ 					imageActionsAlerts($gallery);
/* 567  */ 				});
/* 568  */ 				// emulate pseudo element hover
/* 569  */ 				$gallery.on('mousemove.imageActions', '.gallery > li:not(.disabled) .typebox .fa', function (e) {
/* 570  */ 					var $this = $(this),
/* 571  */ 						width = 29,
/* 572  */ 						height = 26,
/* 573  */ 						left = ($this.outerWidth()-width)/2,
/* 574  */ 						top = ($this.outerHeight()-height)/2;
/* 575  */ 
/* 576  */ 					if(e.offsetX >= left && e.offsetX <= left+width && e.offsetY >= top && e.offsetY <= top+height) {
/* 577  */ 						$this.addClass('hover');
/* 578  */ 					} else {
/* 579  */ 						$this.removeClass('hover');
/* 580  */ 					}
/* 581  */ 				});
/* 582  */ 				$gallery.on('mouseleave.imageActions', '.gallery > li:not(.disabled) .typebox .fa', function () {
/* 583  */ 					$(this).removeClass('hover');
/* 584  */ 				});
/* 585  */ 				// make editing tag removeable
/* 586  */ 				$gallery.on('click.imageActions', '.gallery > li:not(.disabled) .circlebox .tag-editing', function (e) {
/* 587  */ 					var $this = $(this);
/* 588  */ 					e.preventDefault();
/* 589  */ 					e.stopPropagation();
/* 590  */ 					$this
/* 591  */ 						.closest('li').removeClass('editing').end()
/* 592  */ 						.remove();
/* 593  */ 					imageActionsAlerts($gallery);
/* 594  */ 				});
/* 595  */ 				// make contact sheet tag removeable
/* 596  */ 				$gallery.on('click.imageActions', '.gallery > li:not(.disabled) .circlebox .tag-contact-sheet', function (e) {
/* 597  */ 					var $this = $(this);
/* 598  */ 					e.preventDefault();
/* 599  */ 					e.stopPropagation();
/* 600  */ 					$this

/* djd.js */

/* 601  */ 						.closest('li').removeClass('contact-sheet').end()
/* 602  */ 						.remove();
/* 603  */ 					imageActionsAlerts($gallery);
/* 604  */ 				});
/* 605  */ 				// make exposure tag removeable
/* 606  */ 				$gallery.on('click.imageActions', '.gallery > li:not(.disabled) .circlebox .tag-exposure', function (e) {
/* 607  */ 					var $this = $(this).closest('li'),
/* 608  */ 						group = $this.data('exposure-group');
/* 609  */ 					e.preventDefault();
/* 610  */ 					e.stopPropagation();
/* 611  */ 					$this.add($gallery.find('.exposures').filter(function () { return $(this).data('exposure-group') == group })).each(function () {
/* 612  */ 						$(this)
/* 613  */ 							.removeClass('exposures best-exposure').removeData('exposure-group').removeAttr('data-exposure-group')
/* 614  */ 							.find('.tag-exposure, .tag-best-exposure').remove();
/* 615  */ 					});
/* 616  */ 					// don't decrement number of exposure groups,
/* 617  */ 					// as otherwise removing a group that is not the most recent one screws up the counting
/* 618  */ 				});
/* 619  */ 				// untag all button
/* 620  */ 				$gallery.find('.untag-all').click(function (e) {
/* 621  */ 					e.preventDefault();
/* 622  */ 					$gallery.find('.gallery > li:not(.disabled)')
/* 623  */ 						.removeClass('contact-sheet editing exposures'+(pretyped ? '' : ' typed photo floor-plan epc'))
/* 624  */ 						.removeData('exposure-group').removeAttr('data-exposure-group')
/* 625  */ 						.find('.circlebox').empty();
/* 626  */ 					exposure_group = 0;
/* 627  */ 					imageActionsAlerts($gallery);
/* 628  */ 				});
/* 629  */ 				// select all button
/* 630  */ 				$gallery.find('.select-all').click(function (e) {
/* 631  */ 					var $all = $gallery.find('.gallery > li:not(.disabled)');
/* 632  */ 					e.preventDefault();
/* 633  */ 					// if all selected, deselect them
/* 634  */ 					if($all.filter(':not(.selected)').length === 0) {
/* 635  */ 						$all.find('a').trigger('click');
/* 636  */ 					}
/* 637  */ 					// if all are typed, select them
/* 638  */ 					else if($all.filter(':not(.typed)').length === 0) {
/* 639  */ 						$all.filter(':not(.selected)').find('a').trigger('click');
/* 640  */ 					}
/* 641  */ 					// otherwise select all untyped
/* 642  */ 					else {
/* 643  */ 						$all.filter(':not(.selected):not(.typed)').find('a').trigger('click');
/* 644  */ 					}
/* 645  */ 				});
/* 646  */ 				// select images button
/* 647  */ 				$gallery.find('.select-images').click(function (e) {
/* 648  */ 					var $images = $gallery.find('.gallery > li.type-image:not(.disabled)');
/* 649  */ 					e.preventDefault();
/* 650  */ 					// if all images are selected, deselected them

/* djd.js */

/* 651  */ 					if($images.filter(':not(.selected)').length === 0) {
/* 652  */ 						$images.find('a').trigger('click');
/* 653  */ 					}
/* 654  */ 					// if all images are typed, select them
/* 655  */ 					else if($images.filter(':not(.typed)').length === 0) {
/* 656  */ 						$images.filter(':not(.selected)').find('a').trigger('click');
/* 657  */ 					}
/* 658  */ 					// otherwise select all untyped
/* 659  */ 					else {
/* 660  */ 						$images.filter(':not(.selected):not(.typed)').find('a').trigger('click');
/* 661  */ 					}
/* 662  */ 				});
/* 663  */ 				// select documents button
