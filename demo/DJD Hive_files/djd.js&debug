/* 1   */ var DJDNotify = (function () {
/* 2   */ 	"use strict";
/* 3   */ 	var notified = function (nid) {
/* 4   */ 			$('#loading').show();
/* 5   */ 			$.ajax({
/* 6   */ 				async: false,
/* 7   */ 				dataType: 'json',
/* 8   */ 				cache: false,
