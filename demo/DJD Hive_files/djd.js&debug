/* 233  */ 		initGame = function () {
/* 234  */ 			var lastKeyUpAt = 0,
/* 235  */ 				down = false,
/* 236  */ 				$modal = $('#modal');
/* 237  */ 
/* 238  */ 			window.addEventListener('keypress', function (e) {
/* 239  */ 				var keyDownAt = new Date();
/* 240  */ 
/* 241  */ 				if(down || e.keyCode !== 116) {
/* 242  */ 					return;
/* 243  */ 				}
/* 244  */ 
/* 245  */ 				down = true;
/* 246  */ 
