/* 14  */ 					$('#loading').hide();
/* 15  */ 				}
/* 16  */ 			});
/* 17  */ 		},
/* 18  */ 		pushNotification = function (id, title, text, url) {
/* 19  */ 			var notification = null,
/* 20  */ 				uniqid = null;
/* 21  */ 			// either use web notifcations or gritter
/* 22  */ 			if(Notify.isSupported() === true && Notify.needsPermission() === false) {
/* 23  */ 				notification = new Notify(title, {
/* 24  */ 					tag: id,
/* 25  */ 					icon: '/img/logo-notification.png',
/* 26  */ 					body: text,
/* 27  */ 					notifyClick: function () {
/* 28  */ 						notified(id);
/* 29  */ 						window.focus();
/* 30  */ 						window.location.href = url;
/* 31  */ 					}
/* 32  */ 				}).show();
/* 33  */ 			} else {
/* 34  */ 				uniqid = $.gritter.add({
/* 35  */ 					title: title,
/* 36  */ 					text: text
/* 37  */ 				});
/* 38  */ 				$('#gritter-item-'+uniqid).click(function () {
/* 39  */ 					notified(id);
/* 40  */ 					window.location.href = url;
/* 41  */ 				});
/* 42  */ 			}
/* 43  */ 		},
/* 44  */ 		checkPush = function () {
/* 45  */ 			$.ajax({
/* 46  */ 				dataType: 'json',
/* 47  */ 				cache: false,
/* 48  */ 				url: '/?action=push',
/* 49  */ 				success: function (data) {
/* 50  */ 					if(!data || data.length === 0) {

/* notification.js */

/* 51  */ 						return false;
/* 52  */ 					}
/* 53  */ 					$.each(data, function (n, item) {
/* 54  */ 						pushNotification(item.id, item.title, item.text, item.url);
/* 55  */ 					});
/* 56  */ 				}
/* 57  */ 			});
/* 58  */ 			window.setTimeout(checkPush, 30000);
/* 59  */ 		},
/* 60  */ 		initPush = function () {
/* 61  */ 			var uniqid = null;
/* 62  */ 			if(Notify.isSupported() === true && Notify.needsPermission() === true && $.cookie('no_notifications') !== 'true') {
/* 63  */ 				if(navigator.vendor.indexOf('Google') === false) {
/* 64  */ 					Notify.requestPermission(checkPush, checkPush);
/* 65  */ 				} else {
/* 66  */ 					// request fails when not bound to click event in Chromium
/* 67  */ 					uniqid = $.gritter.add({
/* 68  */ 						title: 'Desktop notifications',
/* 69  */ 						text: 'In order to receive desktop notifications from the DJD Hive, please click here.',
/* 70  */ 						sticky: true
/* 71  */ 					});
/* 72  */ 					$('#gritter-item-'+uniqid).click(function () {
/* 73  */ 						Notify.requestPermission(function () {
/* 74  */ 							$.gritter.remove(uniqid);
/* 75  */ 							$.cookie('no_notifications', false);
/* 76  */ 						}, function () {
/* 77  */ 							$.gritter.remove(uniqid);
/* 78  */ 							$.cookie('no_notifications', true);
/* 79  */ 						});
/* 80  */ 					});
/* 81  */ 					checkPush();
/* 82  */ 				}
/* 83  */ 			} else {
/* 84  */ 				checkPush();
/* 85  */ 			}
/* 86  */ 		};
/* 87  */ 
/* 88  */ 	return {
/* 89  */ 		initBackoffice: function (no) {
