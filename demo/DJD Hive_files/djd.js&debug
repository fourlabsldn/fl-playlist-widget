/* 1268 */ 		hoverPreview: function (selector) {
/* 1269 */ 			var $gallery = $(selector),
/* 1270 */ 				height = 0,
/* 1271 */ 				$container;
/* 1272 */ 
/* 1273 */ 			$gallery.find('li').each(function () {
