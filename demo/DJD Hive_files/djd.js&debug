/* 1169 */ 				$phone.removeClass('hidden');
/* 1170 */ 				$inputs.eq(0).removeClass('hidden');
/* 1171 */ 				$inputs.eq(1).addClass('hidden');
/* 1172 */ 			});
/* 1173 */ 		},
/* 1174 */ 		onTabShow: function (selector, infct, outfnct) {
/* 1175 */ 			$('#modal').on('shown.bs.tab', function (e) {
/* 1176 */ 				if(e.target.hash === selector) {
/* 1177 */ 					if(typeof infct === 'function') {
/* 1178 */ 						infct();
/* 1179 */ 					}
/* 1180 */ 				} else {
/* 1181 */ 					if(typeof outfnct === 'function') {
/* 1182 */ 						outfnct();
/* 1183 */ 					}
/* 1184 */ 				}
/* 1185 */ 			});
/* 1186 */ 		},
/* 1187 */ 		initHash: function (addaction) {
/* 1188 */ 			var open_tab = false;
/* 1189 */ 			$(window).on('hashchange', function () {
/* 1190 */ 				var hash = location.hash.substr(1).split('|');
/* 1191 */ 				if(location.hash.length === 0) {
/* 1192 */ 					return false;
/* 1193 */ 				}
/* 1194 */ 				$('#loading').hide();
/* 1195 */ 				$.each(hash, function (n, item) {
/* 1196 */ 					var param = item.split('=');
/* 1197 */ 					switch(param[0]) {
/* 1198 */ 						case 'open':
/* 1199 */ 							$('#modal').modal({
/* 1200 */ 								remote: addaction+param[1]

/* djd.js */

/* 1201 */ 							});
/* 1202 */ 							break;
/* 1203 */ 						case 'open_tab':
/* 1204 */ 							open_tab = param[1];
/* 1205 */ 							break;
/* 1206 */ 						case 'duplicate':
/* 1207 */ 							$('#modal').modal({
/* 1208 */ 								remote: '/?page=form&cat=order&form=order&action=edit&duplicate='+param[1]
/* 1209 */ 							});
/* 1210 */ 							break;
/* 1211 */ 						case 'filter':
/* 1212 */ 							DJDGrid.setFilter(param[1].split(':'));
/* 1213 */ 							break;
/* 1214 */ 						default:
/* 1215 */ 							return 1;
/* 1216 */ 					}
/* 1217 */ 					hash[n] = null;
/* 1218 */ 				});
/* 1219 */ 				location.hash = hash.filter(function(val) { return val !== null; }).join('|');
/* 1220 */ 			}).trigger('hashchange');
/* 1221 */ 			$('#modal').on('loaded.bs.modal', function () {
/* 1222 */ 				$('#modal .nav-tabs a[href=#'+open_tab+']').tab('show');
/* 1223 */ 				open_tab = false;
/* 1224 */ 			});
/* 1225 */ 		},
/* 1226 */ 		calcVat: function (price, vat) {
/* 1227 */ 			var $price = $(price),
/* 1228 */ 				$vat = $(vat);
/* 1229 */ 			$price
/* 1230 */ 				.keyup(function () {
/* 1231 */ 					$vat.val(($price.val()*1.2).toFixed(2));
/* 1232 */ 				})
/* 1233 */ 				.keyup();
/* 1234 */ 		},
