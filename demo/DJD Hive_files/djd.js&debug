/* 774  */ 						var content_visible = $content.visible(true),
/* 775  */ 							prev_visible = $prev.length > 0 && $prev.visible(true);
/* 776  */ 						console.log(content_visible, prev_visible, e.shiftKey, e.keyCode);
/* 777  */ 						if(content_visible === true && prev_visible === false && e.shiftKey === true) {
/* 778  */ 							switch(e.keyCode) {
/* 779  */ 								case 65:
