/* 200  */ 		selectText = function (text) {

/* djd.js */

/* 201  */ 			var range, selection;
/* 202  */ 			if(document.body.createTextRange) {
/* 203  */ 				range = document.body.createTextRange();
/* 204  */ 				range.moveToElementText(text);
/* 205  */ 				range.select();
/* 206  */ 			} else if (window.getSelection) {
/* 207  */ 				selection = window.getSelection();
/* 208  */ 				range = document.createRange();
/* 209  */ 				range.selectNodeContents(text);
/* 210  */ 				selection.removeAllRanges();
/* 211  */ 				selection.addRange(range);
/* 212  */ 			}
