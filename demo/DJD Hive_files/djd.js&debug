/* 736  */ 					});
/* 737  */ 
/* 738  */ 				});
/* 739  */ 				$gallery.find('.action-tag-edit').click(function (e) {
/* 740  */ 					var $selected = $gallery.find('.gallery > .photo.selected:not(.editing)');
