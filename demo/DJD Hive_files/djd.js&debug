/* 879  */ 					}
/* 880  */ 				},
/* 881  */ 				onTabClick: function () {
/* 882  */ 					return false;
/* 883  */ 				}
/* 884  */ 			});
/* 885  */ 			// init uploader
/* 886  */ 			$uploader = DJDUpload.init({
/* 887  */ 				url: '/?page=form&cat=files&form=files&action=upload',
/* 888  */ 				drop_element: 'drop-target',
/* 889  */ 				browse_button: 'drop-target-file',
/* 890  */ 				container: 'formstep-3'
/* 891  */ 			});
/* 892  */ 			if($uploader === false) {
/* 893  */ 				return false;
/* 894  */ 			}
/* 895  */ 
/* 896  */ 			// folder mapping
/* 897  */ 			$uploader.on('djd.uploader.added', function (e, folders) {
/* 898  */ 				var row_html = '<div class="form-group">'+
/* 899  */ 						'<label class="col-sm-3 col-lg-3 control-label">%name%</label>'+
/* 900  */ 						'<div class="col-sm-9 col-lg-9 controls">'+

/* djd.js */

/* 901  */ 							'<select name="folders[]" data-folder="%name%" class="form-control chosen">'+
/* 902  */ 								'<option value="">Please chose project</option>'+
/* 903  */ 							'</select>'+
/* 904  */ 						'</div>'+
/* 905  */ 					'</div>',
/* 906  */ 					html = '',
/* 907  */ 					$options = $('#sample-jobs option');
/* 908  */ 
/* 909  */ 				$wizard.data('bootstrapWizard').next();
/* 910  */ 
/* 911  */ 				folders.sort();
/* 912  */ 				$.each(folders, function (n, folder) {
/* 913  */ 					html += row_html.replace(/%(\w+)%/g, function($0, $1) {
/* 914  */ 						switch ($1) {
/* 915  */ 							case 'name':
/* 916  */ 								return folder;
/* 917  */ 						}
/* 918  */ 					});
/* 919  */ 				});
/* 920  */ 				$('#formstep-2 form')
/* 921  */ 					.prepend(html)
/* 922  */ 					.find('[name="folders[]"]').each(function (n, select) {
/* 923  */ 						$options.clone().appendTo(select);
/* 924  */ 					});
/* 925  */ 				$('#formstep-2 select.chosen:not(:data(chosen))').chosen({
/* 926  */ 					allow_single_deselect: true,
/* 927  */ 					width: "100%"
/* 928  */ 				});
/* 929  */ 			});
/* 930  */ 			$('#formstep-2 button').click(function (e) {
/* 931  */ 				var folder_map = {},
/* 932  */ 					has_error = false,
/* 933  */ 					$error = $('#formstep-2 .alert-danger');
/* 934  */ 				e.preventDefault();
/* 935  */ 				$('#formstep-2').find('[name="folders[]"]').each(function () {
/* 936  */ 					var $this = $(this);
/* 937  */ 					if($this.val() === '') {
/* 938  */ 						$this.closest('.form-group').addClass('has-error');
/* 939  */ 						has_error = true;
/* 940  */ 						return 1;
/* 941  */ 					}
/* 942  */ 					folder_map[$this.data('folder')] = $this.val();
/* 943  */ 				});
/* 944  */ 				if(has_error) {
/* 945  */ 					$error.removeClass('hidden');
/* 946  */ 					$('#modal').animate({scrollTop: 0}, 800, 'swing', function () {
/* 947  */ 						$error.addClass('in');
/* 948  */ 					});
/* 949  */ 					return false;
/* 950  */ 				}

/* djd.js */

/* 951  */ 				DJDUpload.previewFiles(folder_map);
/* 952  */ 				$wizard.data('bootstrapWizard').next();
/* 953  */ 				if(no_typing === false) {
/* 954  */ 					DJD.imageActions('#modal .box:visible');
/* 955  */ 					DJD.hoverTitle('#modal .box:visible');
/* 956  */ 				}
/* 957  */ 			});
/* 958  */ 			$('#formstep-3 button').click(function (e) {
/* 959  */ 				var $progress = $('#modal .circle-progress > input'),
/* 960  */ 					info = {},
/* 961  */ 					$error = $('#formstep-3 .alert-danger');
/* 962  */ 
/* 963  */ 				e.preventDefault();
/* 964  */ 
/* 965  */ 				// do validation
/* 966  */ 				if(no_typing === false && $('#formstep-3 [id^=job-]:not(.hidden) [class*=type-]:not(.typed)').length > 0) {
/* 967  */ 					$error.removeClass('hidden');
/* 968  */ 					$('#modal').animate({scrollTop: 0}, 800, 'swing', function () {
/* 969  */ 						$error.addClass('in');
/* 970  */ 					});
/* 971  */ 					return false;
/* 972  */ 				}
/* 973  */ 
/* 974  */ 				// gather information about jobs and photos
/* 975  */ 				$('#formstep-3 [id^=job-]:not(.hidden)').each(function (n, job) {
/* 976  */ 					var $job = $(job),
/* 977  */ 						oid = parseInt($job.attr('id').match(/\-([0-9]+)/)[1], 10),
/* 978  */ 						comment_editor = $.trim($job.find('[name^=comments-editor]').val()),
/* 979  */ 						comment_client = $.trim($job.find('[name^=comments-client]').val()),
/* 980  */ 						has_cs = false;
/* 981  */ 
/* 982  */ 					if(comment_editor.length > 0) {
/* 983  */ 						comments_editor[oid] = comment_editor;
/* 984  */ 					}
/* 985  */ 					if(comment_client.length > 0) {
/* 986  */ 						comments_client[oid] = comment_client;
/* 987  */ 					}
/* 988  */ 
/* 989  */ 					$job.find('.gallery li').each(function (i, thumb) {
/* 990  */ 						var $thumb = $(thumb),
/* 991  */ 							id = $thumb.attr('id');
/* 992  */ 						info[id] = {
/* 993  */ 							oid: oid,
/* 994  */ 							type: '',
/* 995  */ 							editing: false,
/* 996  */ 							contact_sheet: false,
/* 997  */ 							exposure_group: -1,
/* 998  */ 							best_exposure: false
/* 999  */ 						};
/* 1000 */ 						// type

/* djd.js */

/* 1001 */ 						if($thumb.hasClass('photo')) {
/* 1002 */ 							info[id].type = 'Photo';
/* 1003 */ 						} else if($thumb.hasClass('epc')) {
/* 1004 */ 							info[id].type = 'EPC';
/* 1005 */ 							epcs.push(oid);
/* 1006 */ 						} else if($thumb.hasClass('floor-plan')) {
/* 1007 */ 							info[id].type = 'Floor plan';
/* 1008 */ 						}
/* 1009 */ 						// editing
/* 1010 */ 						if($thumb.hasClass('editing')) {
/* 1011 */ 							info[id].editing = true;
/* 1012 */ 							editing.push(oid);
/* 1013 */ 						}
/* 1014 */ 						// contact sheet
/* 1015 */ 						if($thumb.hasClass('contact-sheet')) {
/* 1016 */ 							info[id].contact_sheet = true;
/* 1017 */ 							contactSheets.push(oid);
/* 1018 */ 							has_cs = true;
/* 1019 */ 						}
/* 1020 */ 						// exposure group
/* 1021 */ 						if($thumb.hasClass('exposures')) {
/* 1022 */ 							info[id].exposure_group = $thumb.data('exposure-group');
/* 1023 */ 						}
/* 1024 */ 						// best exposure
/* 1025 */ 						if($thumb.hasClass('best-exposure')) {
/* 1026 */ 							info[id].best_exposure = true;
/* 1027 */ 						}
/* 1028 */ 					});
/* 1029 */ 
/* 1030 */ 					if(has_cs === true && $job.find('[name^=review_cs]').prop('checked') === true) {
/* 1031 */ 						review_cs.push(oid);
/* 1032 */ 					}
/* 1033 */ 				});
/* 1034 */ 				// move on to the uploader
/* 1035 */ 				$wizard.data('bootstrapWizard').next();
/* 1036 */ 				$progress
/* 1037 */ 					.parent().removeClass('hidden').end()
/* 1038 */ 					.knob({
/* 1039 */ 						'readOnly': true,
/* 1040 */ 						'width': 120,
/* 1041 */ 						'height': 120,
/* 1042 */ 						'dynamicDraw': true,
/* 1043 */ 						'thickness': 0.2,
/* 1044 */ 						'tickColorizeValues': true,
/* 1045 */ 						'skin':'tron'
/* 1046 */ 					});
/* 1047 */ 				$uploader.on('djd.uploader.progress', function (e, percent) {
/* 1048 */ 					$progress.val(percent).trigger('change');
/* 1049 */ 				});
/* 1050 */ 				DJDUpload.start(info);

/* djd.js */

/* 1051 */ 			});
/* 1052 */ 			// upload complete
/* 1053 */ 			$uploader.on('djd.uploader.complete', function (e, failed) {
/* 1054 */ 				var $table;
/* 1055 */ 				// send comments and infos on contact sheets, editing and EPC
/* 1056 */ 				if(epcs.length > 0 || contactSheets.length > 0 || editing.length > 0 ||
/* 1057 */ 						$.isEmptyObject(comments_editor) === false || $.isEmptyObject(comments_client) === false) {
/* 1058 */ 
/* 1059 */ 					$.ajax({
/* 1060 */ 						type: 'post',
/* 1061 */ 						url: '/?page=form&cat=files&form=files&action=postProcessing',
/* 1062 */ 						dataType: 'json',
/* 1063 */ 						data: {
/* 1064 */ 							'contact-sheets': $.unique(contactSheets), // each photo tagged for contact sheet adds its order id
/* 1065 */ 							'editing': $.unique(editing),
/* 1066 */ 							'epcs': $.unique(epcs),
/* 1067 */ 							'comments-editor': comments_editor,
/* 1068 */ 							'comments-client': comments_client,
/* 1069 */ 							'review-cs': review_cs
/* 1070 */ 						}
/* 1071 */ 					});
/* 1072 */ 				}
/* 1073 */ 				if(failed.length > 0) {
/* 1074 */ 					$('#formstep-4').html(
/* 1075 */ 						'<h1 class="text-center"><i class="glyphicon glyphicon-remove text-danger" /></h1>'+
/* 1076 */ 						'<h2 class="text-center">The following files failed to upload:</h2>'+
/* 1077 */ 						'<table class="table table-advance">'+
/* 1078 */ 							'<thead>'+
/* 1079 */ 								'<th>Job #</th>'+
/* 1080 */ 								'<th>File path</th>'+
/* 1081 */ 								'<th>File name</th>'+
/* 1082 */ 							'</thead>'+
/* 1083 */ 							'<tbody />'+
/* 1084 */ 						'</table>'
/* 1085 */ 					);
/* 1086 */ 					$table = $('#formstep-4 table tbody');
/* 1087 */ 					$.each(failed, function() {
/* 1088 */ 						$('<tr><td /><td /><td /></tr>')
/* 1089 */ 							.find('td:eq(0)').text(this.oid).end()
/* 1090 */ 							.find('td:eq(1)').text(this.path).end()
/* 1091 */ 							.find('td:eq(2)').text(this.name).end()
/* 1092 */ 							.appendTo($table);
/* 1093 */ 					});
/* 1094 */ 				} else {
/* 1095 */ 					$('#formstep-4').html(
/* 1096 */ 						'<h1 class="text-center"><i class="glyphicon glyphicon-ok text-success" /></h1>'+
/* 1097 */ 						'<h2 class="text-center">Complete!</h2>'
/* 1098 */ 					);
/* 1099 */ 				}
/* 1100 */ 				DJDGrid.reload();

/* djd.js */

/* 1101 */ 			});
/* 1102 */ 		},
/* 1103 */ 		reload: function (url, selector, callback) {
/* 1104 */ 			$(selector).load(url+' '+selector, function (data) {
/* 1105 */ 				$(this).children(':first').children(':first').unwrap();
/* 1106 */ 				initUI();
/* 1107 */ 				// execute all scripts
/* 1108 */ 				$(data).find(selector+' script').each(function (n, script) {
/* 1109 */ 					if(script.type === 'text/x-tmpl') {
/* 1110 */ 						return 1;
/* 1111 */ 					}
/* 1112 */ 					$(script).appendTo(selector);
/* 1113 */ 				});
/* 1114 */ 				if(typeof callback === 'function') {
/* 1115 */ 					callback();
/* 1116 */ 				}
/* 1117 */ 			});
/* 1118 */ 		},
/* 1119 */ 		csvImport: function (selector) {
/* 1120 */ 			var $form = $(selector),
/* 1121 */ 				$wizard = $form.find('.form-wizard').bootstrapWizard({
/* 1122 */ 					onNext: function (tab, navigation, index) {
/* 1123 */ 						var li_list = navigation.find('li'),
/* 1124 */ 							i;
/* 1125 */ 						li_list.removeClass("done");
/* 1126 */ 						for (i = 0; i < index; i++) {
/* 1127 */ 							$(li_list[i]).addClass("done");
/* 1128 */ 						}
/* 1129 */ 					},
/* 1130 */ 					onTabClick: function () {
/* 1131 */ 						return false;
/* 1132 */ 					}
/* 1133 */ 				});
/* 1134 */ 
/* 1135 */ 			DJD.ajaxForm(selector, function (data) {
