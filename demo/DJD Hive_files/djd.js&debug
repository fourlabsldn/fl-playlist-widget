/* 809  */ 		bulkUpload01: function (oid, type, edited, callback) {
/* 810  */ 			var $uploader, $progress,
/* 811  */ 				type_map = { 1: 'Photo', 2: 'Floor plan', 3: 'EPC', 4: 'Reference' },
/* 812  */ 				info = {};
/* 813  */ 			// init uploader
/* 814  */ 			$uploader = DJDUpload.init({
/* 815  */ 				url: '/?page=form&cat=files&form=files&action=upload',
/* 816  */ 				drop_element: 'drop-target',
/* 817  */ 				browse_button: 'drop-target-file',
/* 818  */ 				required_features: null
/* 819  */ 			});
/* 820  */ 			if($uploader === false) {
/* 821  */ 				return false;
/* 822  */ 			}
/* 823  */ 			$progress = $uploader.find('.circle-progress > input');
/* 824  */ 
/* 825  */ 			$uploader.on('djd.uploader.added', function (e, folders, files) {
/* 826  */ 				$.each(files, function (n, file) {
/* 827  */ 					info[file.id] = {
/* 828  */ 						oid: oid,
/* 829  */ 						type: type_map[type],
/* 830  */ 						edited: edited,
/* 831  */ 						editing: false,
/* 832  */ 						contact_sheet: false,
/* 833  */ 						exposure_group: -1,
/* 834  */ 						best_exposure: 0
/* 835  */ 					};
/* 836  */ 				});
/* 837  */ 				$uploader.find('> span').addClass('hidden');
/* 838  */ 				$progress
/* 839  */ 					.parent().removeClass('hidden').end()
/* 840  */ 					.knob({
/* 841  */ 						'readOnly': true,
/* 842  */ 						'width': 120,
/* 843  */ 						'height': 120,
/* 844  */ 						'dynamicDraw': true,
/* 845  */ 						'thickness': 0.2,
/* 846  */ 						'tickColorizeValues': true,
/* 847  */ 						'skin':'tron'
/* 848  */ 					});
/* 849  */ 				$uploader.on('djd.uploader.progress', function (e, percent) {
/* 850  */ 					$progress.val(percent).trigger('change');

/* djd.js */

/* 851  */ 				});
/* 852  */ 				$uploader.on('djd.uploader.complete', function () {
/* 853  */ 					$('#submodal').modal('hide');
/* 854  */ 					if(typeof callback === 'function') {
/* 855  */ 						callback();
/* 856  */ 					}
/* 857  */ 				});
/* 858  */ 				DJDUpload.start(info);
/* 859  */ 			});
/* 860  */ 		},
/* 861  */ 		bulkUpload02: function (no_typing) {
/* 862  */ 			var $wizard, $uploader,
/* 863  */ 				contactSheets = [],
/* 864  */ 				editing = [],
/* 865  */ 				epcs = [],
/* 866  */ 				comments_editor = {},
/* 867  */ 				comments_client = {},
/* 868  */ 				review_cs = [];
/* 869  */ 
/* 870  */ 			no_typing = no_typing ? true : false;
/* 871  */ 
/* 872  */ 			$wizard = $('#modal .form-wizard').bootstrapWizard({
/* 873  */ 				onNext: function (tab, navigation, index) {
/* 874  */ 					var li_list = navigation.find('li'),
/* 875  */ 						i;
/* 876  */ 					li_list.removeClass("done");
/* 877  */ 					for (i = 0; i < index; i++) {
