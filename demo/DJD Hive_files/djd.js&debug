/* 352  */ 		ajaxForm: function (selector, callback) {
/* 353  */ 			var $form = $(selector);
/* 354  */ 			$form.off('submit.djd').on('submit.djd', function (e) {
/* 355  */ 				e.preventDefault();
/* 356  */ 				if(typeof $form.valid === 'function' && $form.valid() === false) {
/* 357  */ 					return false;
/* 358  */ 				}
/* 359  */ 				$form
/* 360  */ 					.find('[type=submit]').attr('disabled', 'disabled')
/* 361  */ 					.siblings('.spinner-icon').removeClass('hidden')
/* 362  */ 				;
/* 363  */ 				$('#loading').show();
/* 364  */ 				$.ajax({
/* 365  */ 					url: $form.attr('action'),
/* 366  */ 					type: 'post',
/* 367  */ 					cache: false,
/* 368  */ 					contentType: false,
/* 369  */ 					processData: false,
/* 370  */ 					dataType: 'json',
/* 371  */ 					data: new FormData($form.get(0)),
/* 372  */ 					success: function (data) {
/* 373  */ 						$form
/* 374  */ 							.find('[type=submit]').removeAttr('disabled')
/* 375  */ 							.siblings('.spinner-icon').addClass('hidden')
/* 376  */ 						;
/* 377  */ 						$('#loading').hide();
/* 378  */ 						if(typeof data.status !== 'undefined' && data.status) {
/* 379  */ 							$form.find('.alert-success').removeClass('hidden');
/* 380  */ 							if(typeof DJDGrid !== 'undefined') {
/* 381  */ 								DJDGrid.reload();
/* 382  */ 							}
/* 383  */ 							if(typeof DJDCalendar !== 'undefined') {
/* 384  */ 								DJDCalendar.reload();
/* 385  */ 							}
/* 386  */ 						}
/* 387  */ 						if(typeof callback === 'function') {
/* 388  */ 							callback(data);
/* 389  */ 						}
/* 390  */ 					}
/* 391  */ 				});
/* 392  */ 			});
/* 393  */ 		},
/* 394  */ 		ajaxBtn: function (selector, confirmmsg, callback, clickCallback) {
/* 395  */ 			$(selector).off('click.djd').on('click.djd', function (e) {
/* 396  */ 				e.preventDefault();
/* 397  */ 				e.stopPropagation();
/* 398  */ 				if(typeof confirmmsg === 'string' && !confirm(confirmmsg)) {
/* 399  */ 					return false;
/* 400  */ 				}

/* djd.js */

/* 401  */ 				if(typeof clickCallback === 'function') {
/* 402  */ 					clickCallback();
/* 403  */ 				}
/* 404  */ 				$.getJSON($(this).attr('href'), function (data) {
/* 405  */ 					if(typeof confirmmsg === 'function') {
/* 406  */ 						confirmmsg();
/* 407  */ 					}
/* 408  */ 					if(typeof callback === 'function') {
/* 409  */ 						callback(data);
/* 410  */ 					}
/* 411  */ 				});
/* 412  */ 			});
/* 413  */ 		},
/* 414  */ 		password: function (selector) {
