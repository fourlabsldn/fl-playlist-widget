/* djd.js */

/* 1151 */ 		callback: function (selector) {
/* 1152 */ 			var $form = $(selector),
/* 1153 */ 				$phone = $form.find('.fa-phone').parent(),
/* 1154 */ 				$pencil = $form.find('.fa-pencil').parent(),
/* 1155 */ 				$inputs = $form.find('.form-control');
