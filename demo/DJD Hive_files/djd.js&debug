/* 1290 */ 
/* 1291 */ 					$container = $('<div />')
/* 1292 */ 						.append($image)
/* 1293 */ 						.css({
/* 1294 */ 							display: 'block',
/* 1295 */ 							position: 'fixed',
/* 1296 */ 							zIndex: 9999
/* 1297 */ 						})
/* 1298 */ 						.appendTo($('body'));
/* 1299 */ 
/* 1300 */ 				}, function () {

/* djd.js */

/* 1301 */ 					$container.remove();
/* 1302 */ 				});
/* 1303 */ 				$li.mousemove($.throttle(100, function (e) {
/* 1304 */ 					$container.css({
/* 1305 */ 						left: (e.pageX || e.clientX)-30,
/* 1306 */ 						top: (e.pageY || e.clientY)-height-30
/* 1307 */ 					});
/* 1308 */ 				}));
