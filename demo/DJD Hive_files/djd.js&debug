/* 1381 */ 				$start = $(start),
/* 1382 */ 				$end = $(end);
/* 1383 */ 			$starttime.add($endtime).on('change', function () {
/* 1384 */ 				if($starttime.find('option:selected').parent().attr('label').toLowerCase().indexOf('exceptional') === 0 ||
/* 1385 */ 					$endtime.find('option:selected').parent().attr('label').toLowerCase().indexOf('exceptional') === 0) {
/* 1386 */ 
/* 1387 */ 					$end.val($start.val());
/* 1388 */ 					$end.attr('readonly', '');
/* 1389 */ 				} else {
/* 1390 */ 					$end.removeAttr('readonly');
/* 1391 */ 				}
/* 1392 */ 			});
/* 1393 */ 		},
/* 1394 */ 		productGroups: function (groups, extra, sqft) {
/* 1395 */ 			var $groups = $(groups),
/* 1396 */ 				$extra = $(extra);
/* 1397 */ 			$groups.change(function () {
/* 1398 */ 				if($groups.filter(':checked').length === 0) {
/* 1399 */ 					$extra
/* 1400 */ 						.hide()

/* djd.js */

/* 1401 */ 						.find('input:checked').prop('checked', false);
/* 1402 */ 				} else {
/* 1403 */ 					if($extra.css('display') !== 'block') {
/* 1404 */ 						$extra
/* 1405 */ 							.show()
/* 1406 */ 							.find('input:eq(0):not(:checked)').prop('checked', true);
/* 1407 */ 					}
/* 1408 */ 				}
/* 1409 */ 				$groups.each(function () {
/* 1410 */ 					var $this = $(this),
/* 1411 */ 						$sqft = $this.closest('.row').find(sqft);
/* 1412 */ 					if($this.is(':checked')) {
/* 1413 */ 						$sqft.removeClass('hidden');
/* 1414 */ 					} else {
/* 1415 */ 						$sqft.addClass('hidden');
/* 1416 */ 					}
/* 1417 */ 				});
/* 1418 */ 			}).trigger('change');
/* 1419 */ 		},
/* 1420 */ 		setMultipleExposureColours: function (value) {
/* 1421 */ 			colours = value;
/* 1422 */ 		},
/* 1423 */ 		tagCloud: function (selector, tags) {
/* 1424 */ 			$(selector).jQCloud(tags, {
/* 1425 */ 				autoResize: true
/* 1426 */ 			});
/* 1427 */ 			$(window).trigger('resize');
/* 1428 */ 		},
/* 1429 */ 		startGame: function () {
/* 1430 */ 			tetris.init();
/* 1431 */ 
/* 1432 */ 			checkScore();
/* 1433 */ 		}
/* 1434 */ 	};
/* 1435 */ }(jQuery));
